{
    "name": "bassmeets/bitbucketcliphp",
    "description": "PHP CLI application to interact with bitbucket api",
    "keywords": ["framework", "laravel", "laravel zero", "console", "cli"],
    "homepage": "https://github.com/BasSmeets",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "graham-campbell/bitbucket": "^8.0",
        "guzzlehttp/psr7": "^2.0",
        "laravel-zero/framework": "^8.8",
        "php-http/curl-client": "^2.2",
        "php-http/message": "^1.11"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.3",
        "pestphp/pest": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["bb"]
}
